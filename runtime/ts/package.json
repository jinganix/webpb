{
  "name": "webpb",
  "version": "0.0.25",
  "description": "The webpb runtime library",
  "scripts": {
    "build": "rimraf build/**/* && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.cjs && copyfiles ./README.md ./package.json build",
    "check": "npm run lint && npm run test",
    "lint": "eslint --quiet src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "deploy": "npm run check && npm run build && npm publish ./build --access public",
    "test": "vitest --coverage"
  },
  "author": "jinganix@gmail.com",
  "license": "Apache-2.0",
  "keywords": [
    "webpb",
    "runtime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jinganix/webpb"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": [
      {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "./dist/index.cjs"
    ]
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "copyfiles": "^2.4.1",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-quotes": "^0.0.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "prettier": "^3.8.1",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
