{
  "name": "webpb",
  "version": "0.0.16",
  "description": "The webpb runtime library for javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && cp README.md ./dist && cp package.json ./dist",
    "check": "npm run lint && npm run test",
    "lint": "eslint --quiet src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "deploy": "npm run check && npm run build && npm publish ./dist --access public",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles"
  },
  "author": "jinganix@gmail.com",
  "license": "Apache-2.0",
  "keywords": [
    "webpb",
    "runtime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jinganix/webpb"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-quotes": "^0.0.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.2",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
