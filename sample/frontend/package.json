{
  "name": "webpb-frontend",
  "version": "0.0.1",
  "description": "A sample frontend",
  "scripts": {
    "build": "npm run proto && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "check": "npm run proto && npm run lint && npm run test",
    "dev": "webpack serve --config webpack/webpack.config.dev.js",
    "lint": "npm run lint:styles; npm run lint:scripts",
    "lint:scripts": "eslint src",
    "proto": "run-script-os",
    "proto:default": "../../gradlew generateProto",
    "proto:win32": "..\\..\\gradlew.bat generateProto",
    "start": "npm run proto && npm run dev",
    "test": "npm run proto && jest --forceExit --coverage --silent --detectOpenHandles"
  },
  "license": "Apache-2.0",
  "keywords": [
    "webpb",
    "sample",
    "frontend"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.29.0",
    "@eslint/compat": "^2.0.2",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "autoprefixer": "^10.4.24",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.3",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-quotes": "^0.0.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.6",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^28.0.0",
    "mini-css-extract-plugin": "^2.10.0",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.8.1",
    "run-script-os": "^1.1.6",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.7",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "webpack": "^5.105.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3",
    "webpack-merge": "^6.0.1",
    "webpb": "^0.0.25"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.13.5",
    "core-js": "^3.48.0",
    "jest-util": "^30.2.0"
  }
}
