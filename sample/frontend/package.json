{
  "name": "webpb-frontend",
  "version": "0.0.1",
  "description": "A sample frontend",
  "scripts": {
    "build": "npm run proto && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "check": "npm run proto && npm run lint && npm run test",
    "dev": "webpack serve --config webpack/webpack.config.dev.js",
    "lint": "npm run lint:styles; npm run lint:scripts",
    "lint:scripts": "eslint src",
    "lint:styles": "stylelint src",
    "proto": "run-script-os",
    "proto:default": "../../gradlew generateProto",
    "proto:win32": "..\\..\\gradlew.bat generateProto",
    "start": "npm run proto && npm run dev",
    "test": "npm run proto && jest --forceExit --coverage --silent --detectOpenHandles"
  },
  "license": "Apache-2.0",
  "keywords": [
    "webpb",
    "sample",
    "frontend"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.26.0",
    "@eslint/compat": "^1.2.4",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-quotes": "^0.0.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "run-script-os": "^1.1.6",
    "sass": "^1.83.1",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-webpack-plugin": "^5.0.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1",
    "webpb": "^0.0.24"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.7.9",
    "core-js": "^3.40.0"
  }
}
