{
  "name": "webpb-frontend",
  "version": "0.0.1",
  "description": "A sample frontend",
  "scripts": {
    "build": "npm run proto && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "check": "npm run proto && npm run lint && npm run test",
    "dev": "webpack serve --config webpack/webpack.config.dev.js",
    "lint": "npm run lint:styles; npm run lint:scripts",
    "lint:scripts": "eslint --ext .ts --ext .js src",
    "lint:styles": "stylelint src",
    "proto": "run-script-os",
    "proto:default": "../../gradlew generateProto",
    "proto:win32": "../../gradlew.bat generateProto",
    "start": "npm run proto && npm run dev",
    "test": "jest --forceExit --coverage --silent --detectOpenHandles"
  },
  "license": "Apache-2.0",
  "keywords": [
    "webpb",
    "sample",
    "frontend"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "~7.23.0",
    "@babel/plugin-proposal-class-properties": "~7.18.6",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/preset-env": "~7.23.0",
    "@types/axios": "~0.14.0",
    "@types/jest": "~29.5.5",
    "@types/jsdom": "~21.1.3",
    "@typescript-eslint/eslint-plugin": "~6.9.0",
    "@typescript-eslint/parser": "~6.9.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "~9.1.3",
    "clean-webpack-plugin": "~4.0.0",
    "cross-env": "~7.0.3",
    "css-loader": "~6.8.1",
    "eslint": "~8.52.0",
    "eslint-config-prettier": "~9.0.0",
    "eslint-plugin-import": "~2.29.0",
    "eslint-plugin-import-quotes": "~0.0.1",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prettier": "~5.0.0",
    "eslint-plugin-sort-keys-fix": "~1.1.2",
    "file-loader": "~6.2.0",
    "html-loader": "~4.2.0",
    "html-webpack-plugin": "~5.5.3",
    "jest": "~29.7.0",
    "jsdom": "~22.1.0",
    "mini-css-extract-plugin": "~2.7.6",
    "node-sass": "~9.0.0",
    "postcss-loader": "~7.3.3",
    "prettier": "~3.0.3",
    "run-script-os": "~1.1.6",
    "sass-loader": "~13.3.2",
    "style-loader": "~3.3.3",
    "stylelint": "~15.11.0",
    "stylelint-config-standard": "~34.0.0",
    "stylelint-webpack-plugin": "~4.1.1",
    "ts-jest": "~29.1.1",
    "ts-loader": "~9.5.0",
    "typescript": "~5.2.2",
    "webpack": "~5.89.0",
    "webpack-cli": "~5.1.4",
    "webpack-dev-server": "~4.15.1",
    "webpack-merge": "~5.10.0",
    "webpb": "~0.0.16"
  },
  "dependencies": {
    "@babel/polyfill": "~7.12.1",
    "axios": "~1.5.1",
    "core-js": "~3.33.0"
  }
}
